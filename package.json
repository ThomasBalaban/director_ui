{
  "name": "director-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "bash -c 'trap \"kill 0\" EXIT INT TERM; concurrently --kill-others --kill-others-on-fail --names \"ng,launcher\" --prefix-colors \"cyan,magenta\" \"ng serve --port 4200\" \"python launcher.py\" & wait'",    "start:ui": "ng serve --port 4200 --open",
    "start:launcher": "python launcher.py",
    "build": "ng build",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "chart.js": "^4.4.2",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.3",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^20.0.0",
    "concurrently": "^9.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.6.0"
  }
}